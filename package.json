  "dependencies": {
    "@expo/config-plugins": "^2.0.0",
    "base64-js": "^1.1.2",
    "cross-os": "^1.3.0",
    "event-target-shim": "^1.0.5",
    "prop-types": "^15.5.10",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "expo-module-scripts": "^2.0.0"
